    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "recompose": "^0.21.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-devtools": "^3.3.1",
    "three": "^0.83.0"
  },
