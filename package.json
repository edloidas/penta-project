  "repository": {
    "type": "git",
    "url": "git+https://github.com/edloidas/penta-project.git"
  },
  "scripts": {
    "test": "jest",
    "cover": "jest --coverage",
    "report": "node_modules/.bin/jest --coverage && cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "fix": "eslint --fix . || exit 0",
    "prepush": "npm test",
    "postinstall": "npm run download",
    "clean": "runwith ./util/clean public/**/*",
    "download": "node util/tasks/download.js",
    "prod": "npm run clean && cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors --open --content-base public/"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mathjs": "^3.8.1",
