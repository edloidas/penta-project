    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-test-renderer": "^16.5.2",
    "react-transition-group": "^2.5.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "styled-components": "^3.4.9",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-styled-components": "^1.5.0",
    "three": "^0.97.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "yauzl": "^2.10.0"
  },
  "engines": {
    "node": ">= 10.11.0",
    "npm": ">= 6.4.1"
  }
}
