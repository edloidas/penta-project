{
  "name": "penta-project",
  "version": "0.0.1-alpha.2",
  "description": "Action platformer with roguelike elements.",
  "main": "build/index.js",
  "keywords": [
    "game",
    "platformer",
    "roguelike",
    "WebGL",
    "desktop",
    "electron"
  ],
  "homepage": "https://github.com/edloidas/penta-project#readme",
  "bugs": {
    "url": "https://github.com/edloidas/penta-project/issues"
  },
  "license": "GPL-3.0",
  "author": "Edloidas Production <edloidas@gmail.com> (https://edloidas.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edloidas/penta-project.git"
  },
  "scripts": {
    "test": "jest",
    "cover": "jest --coverage",
    "report": "node_modules/.bin/jest --coverage && cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "clean:build": "rimraf build/*.*",
    "clean:dist": "rimraf dist/",
    "clean": "run-p clean:*",
    "typecheck": "flow check",
    "prettier-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint:js": "eslint --fix .",
    "lint:css-only": "stylelint \"src/**/*.css\"",
    "lint:css-in-js": "stylelint \"src/js/ui/components/**/*.js\"",
    "lint:css": "run-s -c lint:css-*",
    "lint": "run-p -c lint:css lint:js",
    "review": "run-p -c test typecheck lint",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "download": "node util/tasks/download.js",
    "build:app": "cross-env NODE_ENV=production webpack --config webpack.app.config.js",
    "build:game": "cross-env NODE_ENV=production webpack --config webpack.game.config.js",
    "build": "npm run clean:build && run-p build:*",
    "dist": "npm run clean:dist && electron-builder --config electron-builder.json",
    "package": "npm run download && run-s clean:* && run-p build:* && build",
    "try:no-build": "electron build/ --enable-logging",
    "try": "webpack --config webpack.app.config.js && npm run try:no-build",
    "dev": "webpack-dev-server --config webpack.game.config.js --inline --hot --progress --colors --history-api-fallback --open --content-base build/"
  },
  "dependencies": {
    "electron-log": "2.2.14",
    "electron-updater": "2.21.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/history": "^4.6.2",
    "@types/lodash.merge": "^4.6.3",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.3.0",
    "autoprefixer": "8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "7.1.4",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "css-mqpacker": "6.0.2",
    "cssnano": "^3.10.0",
    "download": "^6.2.5",
    "electron": "2.0.0-beta.4",
    "electron-builder": "20.5.1",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "0.70.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "mathjs": "4.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "pify": "^3.0.0",
    "postcss-calc": "^6.0.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-normalize": "^4.0.0",
    "postcss-simple-vars": "^4.1.0",
    "prettier": "1.11.1",
    "pug": "2.0.1",
    "pug-loader": "^2.3.0",
    "ramda": "^0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-test-renderer": "16.2.0",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "2.3.0",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "style-loader": "0.20.3",
    "styled-components": "3.2.3",
    "stylelint": "9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-styled-components": "1.3.1",
    "three": "^0.91.0",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.2",
    "yauzl": "^2.9.1"
  },
  "engines": {
    "node": ">= 8.9.1",
    "npm": ">= 5.5.1",
    "yarn": ">= 1.3.2"
  }
}
