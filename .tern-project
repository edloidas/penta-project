{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "*.js",
    "src/**/*.js",
    "util/**/*.js"
  ],
  "plugins": {
    "doc_comment": {},
    "complete_strings": {},
    "node": {},
    "requirejs": {
      "baseURL": "./",
      "paths": {}
    },
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true
    }
  }
}
