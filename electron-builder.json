{
  "appId": "com.edloidas.penta-project",
  "productName": "Penta Project",
  "asar": true,
  "compression": "normal",
  "directories": {
    "buildResources": "build",
    "output": "dist"
  },
  "files": [
    "build"
  ],
  "win": {
    "target": [
      "nsis",
      "zip"
    ],
    "icon": "build/data/logo/icon.ico",
    "publish": "github"
  },
  "mac" : {
    "icon": "build/data/logo/icon.icns"
  },
  "linux": {
    "target": [
      "AppImage",
      "tar.gz",
      "deb"
    ],
    "icon": "build/data/logo/"
  }
}
